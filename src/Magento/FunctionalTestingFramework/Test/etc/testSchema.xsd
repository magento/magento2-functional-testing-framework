            <xs:element type="waitForLoadingMaskToDisappearType" name="waitForLoadingMaskToDisappear" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element type="waitForPageLoadType" name="waitForPageLoad" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element type="waitForTextType" name="waitForText" minOccurs="0" maxOccurs="unbounded"/>
        </xs:choice>
    </xs:group>

    <!-- Action Group ref -->

    <xs:complexType name="actions">
        <xs:sequence maxOccurs="unbounded" minOccurs="0">
            <xs:element name="argument">
                <xs:complexType>
                    <xs:attribute type="xs:string" name="name"/>
                    <xs:attribute type="xs:string" name="value"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute type="xs:string" name="ref" use="required"/>
        <xs:attribute type="xs:string" name="mergeKey" use="required"/>
        <xs:attribute type="xs:boolean" name="remove" default="false"/>
        <xs:attribute type="xs:string" name="before"/>
        <xs:attribute type="xs:string" name="after"/>
    </xs:complexType>

    <!-- Test action types -->

    <xs:complexType name="acceptPopupType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute type="xs:string" name="mergeKey"/>
                <xs:attribute type="xs:boolean" name="remove" default="false"/>
                <xs:attribute type="xs:string" name="before"/>
                <xs:attribute type="xs:string" name="after"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="amOnPageType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute type="xs:string" name="url"/>
                <xs:attribute type="xs:string" name="variable"/>
                <xs:attribute type="xs:string" name="mergeKey" use="required"/>
                <xs:attribute type="xs:boolean" name="remove" default="false"/>
                <xs:attribute type="xs:string" name="before"/>
                <xs:attribute type="xs:string" name="after"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
