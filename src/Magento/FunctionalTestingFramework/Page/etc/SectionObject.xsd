<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:redefine schemaLocation="mergedSectionObject.xsd">
        <xs:complexType name="SectionType">
            <xs:sequence>
                <xs:element ref="section" maxOccurs="1" minOccurs="1">
                    <xs:annotation>
                        <xs:documentation>
                            Contains sequence of ui elements in a section of a page.
                        </xs:documentation>
