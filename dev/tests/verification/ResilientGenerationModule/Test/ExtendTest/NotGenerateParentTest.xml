<tests xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/testSchema.xsd">
    <test name="NotGenerateParentTest">
        <annotations>
            <severity value="AVERAGE"/>
            <title value="NotGenerateParentTest"/>
            <features value="NotGenerateParentTest"/>
            <stories value="NotGenerateParentTest"/>
        </annotations>
        <before>
            <amOnPage url="{{NOTaPAGE.url}}" stepKey="beforeAmOnPageKey"/>
        </before>
        <after>
            <amOnPage url="/afterUrl" stepKey="afterAmOnPageKey"/>
        </after>
        <comment stepKey="basicCommentWithNoData" userInput="Parent Comment"/>
        <amOnPage url="/url/in/parent" stepKey="amOnPageInParent"/>
    </test>
</tests>
