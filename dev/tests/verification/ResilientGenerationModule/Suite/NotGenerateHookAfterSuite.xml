<suites xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:mftf:Suite/etc/suiteSchema.xsd">
    <suite name="NotGenerateHookAfterSuite">
        <include>
            <test name="IncludeTest"/>
        </include>
        <before>
            <amOnPage url="some.url" stepKey="before"/>
        </before>
        <after>
            <actionGroup ref="actionGroupWithTwoArgumentsN" stepKey="AC">
                <argument name="somePerson" value="simpleData"/>
                <argument name="anotherPerson" value="uniqueData"/>
            </actionGroup>
        </after>
    </suite>
</suites>
